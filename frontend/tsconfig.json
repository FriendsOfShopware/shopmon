{
  "compilerOptions": {
    "strict": true,
    "baseUrl": "src",
    "target": "esnext",
    "moduleResolution": "Node",
    "module": "ES2020",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "strictNullChecks": true,
    "lib": [
      "DOM",
      "ES2015",
      "ESNext"
    ],
    "jsx": "preserve",
    "types": [
      "unplugin-icons/types/vue",
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "test"
  ]
}
